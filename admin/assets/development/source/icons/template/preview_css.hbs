@font-face {
    font-family: "{{fontName}}";
    src: url('{{fontPath}}{{fontName}}.eot?ts={{fontVersion}}');
    src: url('{{fontPath}}{{fontName}}.eot?ts={{fontVersion}}#iefix') format('eot'),
    url('{{fontPath}}{{fontName}}.woff2?ts={{fontVersion}}') format('woff2'),
    url('{{fontPath}}{{fontName}}.woff?ts={{fontVersion}}') format('woff'),
    url('{{fontPath}}{{fontName}}.ttf?ts={{fontVersion}}') format('truetype'),
    url('{{fontPath}}{{fontName}}.svg?ts={{fontVersion}}#{{fontName}}') format('svg');
}

[class^="{{fontPrefix}}-"]::before, [class*=" {{fontPrefix}}-"]::before {
    font-family: "{{fontName}}";
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    speak: none;
    text-decoration: none;
    text-transform: none;
}

{{#each fontGlyphs}}
.{{../fontPrefix}}-{{@key}}::before {
    content: "{{this}}";
}
{{/each}}